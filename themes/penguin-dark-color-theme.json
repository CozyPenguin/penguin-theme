{
	"name": "Cozy Theme",
	"type": "dark",
	"semanticHighlighting": true,
	"colors": {
		"editor.background": "#1d1d1d",
		"editor.foreground": "#fffffd",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#fffffd",
		// status bar
		"statusBar.border": "#1d1d1d",
		"statusBar.foreground": "#b1b1b1",
		"statusBarItem.remoteBackground": "#007acc",
		"statusBarItem.remoteForeground": "#fffffd",
		// title bar
		"titleBar.activeBackground": "#1f1f1f",
		"titleBar.activeForeground": "#b1b1b1",
		"titleBar.inactiveBackground": "#4b4b4b",
		"titleBar.inactiveForeground": "#cccccc",
		// tabs
		"tab.activeForeground": "#fffffd",
		// terminal
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBrightBlack": "#121212",
		"terminal.ansiBlue": "#007acc",
		"terminal.ansiBrightBlue": "#3197db",
		"terminal.ansiCyan": "#2cc1cc",
		"terminal.ansiBrightCyan": "#83DAE0",
		"terminal.ansiGreen": "#1FCF86",
		"terminal.ansiBrightGreen": "#00ff62",
		"terminal.ansiMagenta": "#c220ba",
		"terminal.ansiBrightMagenta": "#ff6af8",
		"terminal.ansiRed": "#e43939",
		"terminal.ansiBrightRed": "#F87474",
		"terminal.ansiWhite": "#fffffd",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiYellow": "#e6e927",
		"terminal.ansiBrightYellow": "#F8FA74",
		// git decorations
		"gitDecoration.modifiedResourceForeground": "#6a9dbe",
		"gitDecoration.addedResourceForeground": "#1FCF86",
		"gitDecoration.deletedResourceForeground": "#e64f70",
		// dif editor
		"diffEditor.insertedTextBackground": "#45ffb21c",
		"diffEditor.removedTextBackground": "#e439391c",
		//problem icon colours
		"problemsErrorIcon.foreground": "#e64f70",
		"problemsWarningIcon.foreground": "#caae11",
		"problemsInfoIcon.foreground": "#007acc"

	},
	"tokenColors": [{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#89969c"
			}
		},
		{
			"name": "Control Keyword",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#1fcf86",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Escape Character",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#60faba"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"entity.name.method"
			],
			"settings": {
				"foreground": "#a09fff",
			}
		},
		{
			"name": "Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#e64f70",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup bold",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup italic",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Modifier",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#d19ff3"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#5ddfe4",
			}
		},
		{
			"name": "Operator",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#59afff"
			}
		},
		{
			"name": "Other Keyword",
			"scope": [
				"keyword.other",
				"meta.preprocessor",
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#f87474"
			}
		},
		{
			"name": "Property",
			"scope": [
				"variable.name.other.property",
				"entity.name.variable.property.cs",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#cefac1"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string",
				"punctuation.definition.char",
				"punctuation.terminator",
				"punctuation.curlybrace",
				"punctuation.parenthesis",
				"punctuation."
			],
			"settings": {
				"foreground": "#83dae0",
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#b8ce7c"
			}
		},
		{
			"name": "Name",
			"scope": [
				"storage.type",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#fabc36"
			}
		},
		{
			"name": "Type Language Constant",
			"scope": [
				"keyword.type",
				"constant.language"
			],
			"settings": {
				"foreground": "#96c2fc"
			}
		}
	],
	"semanticTokenColors": {
		"enumMember": "#fdffe4",
		"field": "#f8fa74",
		"local": "#fdffe4",
		"number": "#5ddfe4",
		"operator": "#59afff",
		"parameter": "#f8a251",
		"property": "#cefac1",
		"xmlDocCommentDelimiter": "#585e64",
		"xmlDocCommentText": "#727980",
		"xmlDocCommentName": "#4378ad",
		"stringEscapeCharacter": "#60faba"
	}
}